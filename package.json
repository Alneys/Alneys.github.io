{
  "name": "alneys-github-pages",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --build --force",
    "preview": "vite preview",
    "test:unit": "vitest",
